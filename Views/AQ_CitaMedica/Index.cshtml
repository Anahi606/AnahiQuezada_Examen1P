@model IEnumerable<AnahiQuezada_Examen1P.Models.AQ_CitaMedica>

@{
    ViewData["Title"] = "Creado por Anahi Quezada";
}

<h1>Creado por Anahi Quezada</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr bgcolor="Gray">
            <th>
                @Html.DisplayNameFor(model => model.AQ_Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AQ_Urgente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AQ_Precio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AQ_Fecha)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AQ_Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AQ_Urgente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AQ_Precio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AQ_Fecha)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AQ_Id"> <input type ="submit" value="Editar" class="btn btn-primary" /></a> |
                    <a asp-action="Details" asp-route-id="@item.AQ_Id"> <input type="submit" value="Detalles" class="btn btn-primary" /></a> |
                    <a asp-action="Delete" asp-route-id="@item.AQ_Id"> <input type="submit" value="Eliminar" class="btn btn-danger" /></a> |
            </td>
        </tr>
}
    </tbody>
</table>
